pipeline 
{ 
    agent any
        stages{
            stage('one')
            {
              steps
              {
                  echo "hello world"  
            }
        }
        stage('two')
        {
            steps
            { 
                sh "java -version"
        }
    }
    stage ('three')
    {
        steps
        {
            input ("do you want to proceed ")
        }
    }
}
}
